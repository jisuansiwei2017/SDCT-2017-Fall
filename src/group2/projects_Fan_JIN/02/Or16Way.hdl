/**
 * 16-way Or: 
 * out = (in[0] or in[1] or ... or in[15])
 */

CHIP Or16Way {
	IN in[16];
	OUT out;
	
	PARTS:
	Or8Way(in = in[0..7], out = temp1);
	Or8Way(in = in[8..15], out = temp2);
	Or(a = temp1, b = temp2, out = out);
}